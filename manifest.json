{
  "name": "NAYIM FBState",
  "version": "2.0",
  "description": "Import/export fbstate.json which can be used to let bot login.",
  "permissions": [
    "cookies",
    "activeTab",
    "https://*.facebook.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo/16.jpeg",
      "32": "logo/32.jpeg",
      "48": "logo/48.jpeg",
      "64": "logo/64.jpeg",
      "128": "logo/128.jpeg"
    }
  },
  "icons": {
    "16": "logo/16.jpeg",
    "32": "logo/32.jpeg",
    "48": "logo/48.jpeg",
    "64": "logo/64.jpeg",
    "128": "logo/128.jpeg"
  },
  "manifest_version": 2
}
 
